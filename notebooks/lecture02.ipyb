   "source": [
    "# CASE文を使った整形\n",
    "# as を使ったカラム名の変更\n",
    "#　性別ごとの登録ユーザ\n",
    "\n",
    "sql = \"\"\"\n",
    "SELECT\n",
    "    CASE \n",
    "        WHEN gender_id = 0 THEN 'male'\n",
    "        WHEN gender_id = 1 THEN 'female'\n",
    "        ELSE 'unknown'\n",
    "    END as gender,\n",
    "    COUNT(user_id) as number_of_user\n",
    "FROM\n",
    "    users\n",
    "GROUP BY\n",
    "    gender_id\n",
    "\"\"\"\n",
    "\n",
    "pandas.read_sql(sql, sqlite_conn)"
   ]
