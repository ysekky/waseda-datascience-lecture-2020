   "source": [
    "# 日付の扱い方、\n",
    "# strftimeで日付型を文字型に変換\n",
    "# %Y -> 年\n",
    "# %w  曜日: 0-6 (日曜日が0、土曜日が6)\n",
    "# %H  時: 00-24\n",
    "# %M  分: 00-59\n",
    "# %S  秒: 00-59\n",
    "\n",
    "sql = \"\"\"\n",
    "SELECT\n",
    "    STRFTIME(\"%Y-%m-%d\", created_at) as created_date\n",
    "FROM\n",
    "    users\n",
    "LIMIT 10\n",
    "\"\"\"\n",
    "\n",
    "pandas.read_sql(sql, sqlite_conn)"
   ]
